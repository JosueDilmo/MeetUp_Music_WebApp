generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "sqlite"
  url      = env("DATABASE_URL")
}

model User {
  userId     String @id
  firstName  String
  lastName   String
  profession String

  events Events[]
}

model Events {
  eventId   String   @id @default(uuid())
  ownerId   String
  joinedId  String?
  latitude  Float
  longitude Float
  createdAt DateTime @default(now())
  hourStart Int
  hourEnd   Int
  user      User     @relation(fields: [ownerId], references: [userId])
}
